{
    "name": "elios_backend",
    "version": "0.0.1",
    "private": true,
    "license": "UNLICENSED",
    "files": ["dist"],
    "main": "dist/main.js",
    "types": "dist/main.d.ts",
    "scripts": {
        "build": "nest build && npm run build:path",
        "build:path": "resolve-tspaths",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "dev": "yarn start:dev",
        "start:dev": "nest start --watch",
        "start:worker": "ts-node ./src/main-worker.ts",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "yarn biome check --write --config-path=./biome.json .",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare": "husky install",
        "commit": "cz",
        "genDocu": "npx @compodoc/compodoc -p ./tsconfig.json -n 'Elios backend documentation'",
        "serveDocu": "npx @compodoc/compodoc -s",
        "seed": "ts-node ./src/seed/main",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/database/data-source.ts",
        "migration:generate": "yarn typeorm migration:generate -n",
        "migration:run": "yarn typeorm migration:run",
        "migration:revert": "yarn typeorm migration:revert"
    },
    "dependencies": {
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@nestjs/axios": "^3.0.2",
        "@nestjs/bullmq": "^11.0.2",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.2.3",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/schedule": "^6.0.0",
        "@nestjs/swagger": "^7.4.2",
        "@nestjs/typeorm": "^10.0.2",
        "@types/nodemailer": "^6.4.17",
        "@willsoto/nestjs-prometheus": "^6.0.2",
        "axios": "^1.7.7",
        "bcrypt": "^5.1.1",
        "bullmq": "^5.44.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "commitizen": "^4.3.0",
        "dotenv": "^16.4.7",
        "elios_backend": "file:",
        "google-auth-library": "^9.15.1",
        "ioredis": "^5.6.0",
        "nodemailer": "^7.0.5",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.12.0",
        "prom-client": "^15.1.3",
        "reflect-metadata": "^0.2.0",
        "resolve-tspaths": "^0.8.23",
        "rxjs": "^7.8.1",
        "stripe": "^18.0.0",
        "typeorm": "^0.3.20",
        "uuid": "^11.1.0",
        "xstate": "^5.19.2"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.2",
        "@commitlint/cz-commitlint": "^19.5.0",
        "@commitlint/is-ignored": "^19.5.0",
        "@compodoc/compodoc": "^1.1.25",
        "@faker-js/faker": "^9.3.0",
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/passport": "^1.0.17",
        "@types/passport-jwt": "^4.0.1",
        "@types/supertest": "^6.0.0",
        "@types/uuid": "^10.0.0",
        "husky": "^8.0.0",
        "jest": "^29.5.0",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": ["js", "json", "ts"],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": ["**/*.(t|j)s"],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
